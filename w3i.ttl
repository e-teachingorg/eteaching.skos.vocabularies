w3i:
    a skos:ConceptScheme;
    dct:title "Problemfelder"@de ;
    skos:hasTopConcept w3i:2677c, w3i:83f3e, w3i:b44d8, w3i:d5182, w3i:6588c, w3i:721d4, w3i:839e9 .
w3i:2677c
    a skos:Concept ;
    skos:prefLabel "Didaktische Handlungsfelder der Lehre"@de ;
    skos:example "Lehre planen und durchführen, Prüfen, Beraten und Betreuen, Lehre entwickeln, Lehre evaluieren"@de ;
    skos:topConceptOf w3i: .
w3i:83f3e
    a skos:Concept;
    skos:prefLabel "Persönliche/strukturelle Probleme"@de ;
