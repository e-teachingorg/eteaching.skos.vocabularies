    skos:topConceptOf w3i: .
w3i:b44d8
    a skos:Concept;
    skos:prefLabel "Austausch und Vernetzung"@de ;
    skos:example "Hochschulübergreifende Netzwerke, Vernetzung, Austausch, Kollaboration, Communitybuilding, Austauschforen/Innovationslabore"@de ;
    skos:topConceptOf w3i: .
w3i:d5182
    a skos:Concept;
    skos:prefLabel "Support-Strukturen"@de ;
